# Backend environment example
# Required: Postgres / Neon connection string
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DATABASE_NAME

# Frontend origin (used for CORS and Better Auth trusted origins)
FRONTEND_URL=http://localhost:5173

# Secret used by Better Auth (set a long random value in production)
BETTER_AUTH_SECRET=change_this_to_a_secure_random_value

# Node environment
NODE_ENV=development
